<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - NoResume</title>

    <environment include="Development">
        <link rel="stylesheet" href="~/lib/materialize/css/materialize.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="~/lib/materialize/css/materialize.min.css"/>
    </environment>
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Poiret+One|Quicksand&display=swap" rel="stylesheet">
    <link rel='stylesheet' href='https://use.fontawesome.com/releases/v5.7.0/css/all.css'
            integrity='sha384-lZN37f5QGtY3VHgisS14W3ExzMWZxybE1SJSEsQp9S+oqd12jhcu+A56Ebc1zFSJ' crossorigin='anonymous'>
    <link rel="stylesheet" href="~/css/site.css">
    <link rel="stylesheet" href="~/chartjs/chart.min.css" >
</head>
<body>
<header>
    <nav id="navbarMaterialize" class="teal darken-2">
        <div class="nav-wrapper">
            <a asp-area="" asp-controller="Home" asp-action="Index" class="brand-logo" id="page-logo">
                <i class="material-icons" id="page-logo-icon">layers_clear</i> noResume
            </a>
    
            <a href="#" class="sidenav-trigger" data-target="mobile-nav">
                <i class="material-icons">menu</i>
            </a>
            <ul class="right hide-on-med-and-down ">
                <li>
                    <a class="tooltipped" data-position="bottom" data-tooltip="GitHub" target="_blank"
                       href="https://github.com/shunjid/NoResume.git">
                        <i class="fab fa-github"></i>
                    </a>
                </li>
                <li>
                    <a class="" asp-action="Login" asp-controller="Account">Login</a>
                </li>
            </ul>
        </div>
    </nav>
    <ul class="sidenav" id="mobile-nav">
        <li>
            <a target="_blank" class="title" href="https://github.com/shunjid/NoResume.git">
                <i class="grey-text text-darken-4 fab fa-github"></i> GitHub
            </a>
        </li>
    </ul>
</header>
    <div id="intLoader" class="progress">
      <div class="indeterminate"></div>
    </div>
    <div class="section">
        <main role="main" class="main">
            @RenderBody()
        </main>
    </div>
    
    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2019 - no_resume - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>

    <environment include="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/lib/materialize/js/materialize.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/materialize/js/materialize.min.js"></script>
    </environment>
    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>