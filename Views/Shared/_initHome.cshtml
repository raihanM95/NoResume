        function showUserDefinedToast(e,n){M.toast({html:e,classes:n})}$(document).ready(function(){showUserDefinedToast("Welcome back","green darken-1 rounded"),setTimeout(function(){showUserDefinedToast("Total visited : "+numberOfUser,"deep-purple darken-1 rounded")},6e3)});
    </script>
    <script src="~/chartjs/chart.min.js" asp-append-version="true"></script>
    <script src="~/api/app.min.js" asp-append-version="true"></script>
</environment>